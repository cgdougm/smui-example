    "@material/ripple": "^13.0.0",
    "@material/rtl": "^13.0.0",
    "@material/shape": "^13.0.0",
    "@material/theme": "^13.0.0",
    "@material/tokens": "^13.0.0",
    "@material/touch-target": "^13.0.0",
    "@material/typography": "^13.0.0",
    "cookie": "^0.4.1"
  }
